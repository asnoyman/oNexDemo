{
  "name": "onex-platform",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "db:generate": "drizzle-kit generate:pg --out ./drizzle/migrations --schema ./src/schema.js",
    "db:push": "drizzle-kit push --dialect postgresql --schema ./src/schema.js",
    "db:studio": "drizzle-kit studio",
    "init-db": "node init-db.js",
    "start": "node index.js",
    "dev": "nodemon index.js"
  },
  "dependencies": {
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.43.1",
    "pg": "^8.16.0"
  },
  "devDependencies": {
    "@types/pg": "^8.15.2",
    "drizzle-kit": "^0.31.1",
    "nodemon": "^3.0.3"
  }
}
