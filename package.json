{
  "name": "onex-platform",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "db:generate": "drizzle-kit generate:pg --out ./drizzle/migrations --schema ./src/schema.js",
    "db:push": "drizzle-kit push --dialect postgresql --schema ./src/schema.js",
    "db:studio": "drizzle-kit studio",
    "init-db": "node init-db.js",
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "dependencies": {
    "@apollo/server": "^4.10.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.43.1",
    "express": "^4.18.2",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.0"
  },
  "devDependencies": {
    "@types/pg": "^8.15.2",
    "drizzle-kit": "^0.31.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "supertest": "^6.3.4",
    "tsx": "^4.19.4"
  }
}
